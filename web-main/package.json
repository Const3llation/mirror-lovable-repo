		"@radix-ui/react-slot": "^1.2.0",
		"@radix-ui/react-tooltip": "^1.2.4",
		"@rumess/payload-audit-log": "^1.0.1",
		"@tanstack/react-query": "^5.75.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"countries-list": "^3.1.1",
		"cross-env": "^7.0.3",
		"downshift": "^9.0.9",
		"ioredis": "^5.6.1",
		"klaviyo-api": "16.0.0",
		"lucide-react": "0.475.0",
		"motion": "^12.9.4",
